<h1>Listing settings</h1>

<table>
  <tr>
  </tr>

<% @settings.each do |setting| %>
  <tr>
    <td><%= h setting.key %></td>
    <td><%= h setting.value %></td>
    <td><%= link_to 'Show', admin_setting_path(setting) %></td>
    <td><%= link_to 'Edit', edit_admin_setting_path(setting) %></td>
    <td><%= link_to 'Destroy', admin_setting_path(setting), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New setting', new_admin_setting_path %>
